Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"sol2", "[", 
    RowBox[{
    "n_", ",", "M_", ",", "f_", ",", "\[Tau]_", " ", ",", "\[Delta]1_", ",", 
     "\[Delta]2_", ",", "\[Alpha]1X_", ",", "\[Alpha]1Y_", ",", "\[Alpha]2X_",
      ",", "\[Alpha]2Y_"}], "]"}], " ", "=", "\[IndentingNewLine]", 
   RowBox[{"Solve", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"\[CapitalDelta]s", "[", 
        RowBox[{
        "n", ",", "\[Delta]1", ",", "\[Alpha]1X", ",", "\[Alpha]1Y", ",", 
         "n1X", ",", "n1X", ",", "n1Y"}], "]"}], " ", "\[Equal]", "0"}], "&&",
       "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"\[CapitalDelta]s", "[", 
        RowBox[{
        "n", ",", "\[Delta]2", ",", "\[Alpha]2X", ",", "\[Alpha]2Y", ",", 
         "n2X", ",", "n2X", ",", "n2Y"}], "]"}], " ", "\[Equal]", "0"}], " ", 
      "&&", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"\[CapitalDelta]nX2", "[", 
        RowBox[{
        "n", ",", "n1X", ",", "n2X", ",", "S1", ",", "S2", ",", "M", ",", "M",
          ",", "f", ",", "\[Tau]"}], "]"}], "\[Equal]", "0"}], "&&", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"\[CapitalDelta]nX2", "[", 
        RowBox[{
        "n", ",", "n2X", ",", "n1X", ",", "S2", ",", "S1", ",", "M", ",", "M",
          ",", "f", ",", "\[Tau]"}], "]"}], "\[Equal]", "0"}], " ", "&&", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"\[CapitalDelta]nY2", "[", 
        RowBox[{
        "n", ",", "n1Y", ",", "n2Y", ",", "S1", ",", "S2", ",", "M", ",", "M",
          ",", "f", ",", "\[Tau]"}], "]"}], "\[Equal]", "0"}], "&&", 
      "\[IndentingNewLine]", 
      RowBox[{
